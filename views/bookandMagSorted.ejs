<!DOCTYPE html>
<html lang="en">
<head>
    <title>Document</title>
</head>

<body>
    <div> BOOKS AND MAGZINES SORTED </div>
    <% for(var i = 0;i< data.length ;i++) { %>
        <% title = data[i].title %>
        <% isbn = data[i].isbn %>
        <% authors = data[i].authors %>
        <% pub = data[i].publishedAt %>
        <% desc = data[i].description %>

         <div> Title : <%=title %> </div>
         <div> ISBN  : <%=isbn %> </div>
         <div> Authors : <%=authors %> </div>
         <div> publishedAt : <%=pub %> </div>
         <div> Description : <%=desc %> </div>
         <br>
         <br>

   <% } %>
  
   
</body>
</html>